package CSE;

public class thread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  Thread t=new Thread();
	       t.start();
	        System.out.println("thread is running inside");
		}
		public void run()
		{
			System.out.println("thread is running outside");
		}
				
		}
		
		

	
